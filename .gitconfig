[user]
	name = OKADA Shohei
	email = okashoicircus0@gmail.com
[init]
	defaultBranch = main
[core]
	filemode = false
	excludesfile = ~/.gitignore-global
	autocrlf = false
[color]
	ui = true
[alias]
	st = status
	b = branch
	ba = branch -a
	co = switch
	cob = switch -c
	cm = commit
	cmm = commit -m
	cma = commit --amend
	cmam = commit --amend -m
	diffc = diff --cached
	graph = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
	f = fetch
	fp = fetch --prune
	s = switch
[includeIf "gitdir:~/Projects/src/bitbucket.org/willgateteamvietnum/"]
	path = ~/Projects/src/bitbucket.org/willgateteamvietnum/.gitconfig
[includeIf "gitdir:~/Projects/src/github.com/willgate-dev/"]
	path = ~/Projects/src/github.com/willgate-dev/.gitconfig
[includeIf "gitdir:~/Projects/src/github.com/linkage-corp/"]
	path = ~/Projects/src/github.com/linkage-corp/.gitconfig
[ghq]
	root = ~/Projects/src
[pull]
	rebase = false
